{
  "name": "hta-errors",
  "version": "1.0.0",
  "author": "Big Blue Door",
  "homepage": "https://www.hta.gov.uk",
  "description": "HTA Error pages / sitemap",
  "scripts": {
    "build": "node-sass -r scss -o css --output-style=compressed",
    "build-prod": "node-sass -r scss -o css --output-style=compressed && rm -R node_modules",
    "watch": "node-sass -wr scss -o css --output-style=compressed"
  },
  "dependencies": {
    "bulma": "^0.9.2",
    "destyle.css": "^2.0.2",
    "node-sass": "^4.13.1",
    "scss-compile": "^0.1.7"
  }
}
